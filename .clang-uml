compilation_database_dir: .
output_directory: .
add_compile_flags:
  - -D_CLANG_UML_
diagrams:
  include_diagram:
    type: include
    title: Includes diagram
    glob:
      - PB/src/*
    generate_system_headers: false
    using_namespace: PB
    include:
      paths:
        - PB/include
        - PB/src
    exclude:
      namespaces:
        - std
  
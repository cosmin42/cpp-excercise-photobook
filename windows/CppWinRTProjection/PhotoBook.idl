namespace CppWinRTProjection {

interface GradualControllableListener {
  void OnFinished();
  void OnStopped();
  void OnStarted();
  void OnPaused();
  void OnResumed();

  void OnProgressUpdate();
};

[default_interface] runtimeclass PhotoBook {
  PhotoBook(GradualControllableListener listener);
  void AddMedia(String path);
  void setOutputPath(String path);
}
} // namespace CppWinRTProjection
